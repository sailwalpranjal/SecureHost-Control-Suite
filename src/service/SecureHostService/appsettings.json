{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "SecureHostService": "Information",
      "SecureHostCore": "Information"
    },
    "EventLog": {
      "SourceName": "SecureHostService",
      "LogName": "Application",
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning"
      }
    }
  },
  "SecureHost": {
    "ServiceName": "SecureHostService",
    "ApiPort": 5555,
    "ApiBindAddress": "127.0.0.1",
    "DataPath": "%ProgramData%\\SecureHost",
    "AuditPath": "%ProgramData%\\SecureHost\\Audit",
    "ConfigPath": "%ProgramData%\\SecureHost\\Config",
    "DefaultPolicy": "Block",
    "EnableKernelEnforcement": true,
    "EnableTamperProtection": true,
    "EnableAuditLogging": true,
    "AuditRetentionDays": 90,
    "MaxAuditFileSize": 104857600
  }
}
